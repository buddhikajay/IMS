{% extends 'base.html.twig' %}

{% block body %}
    <h1>InquiryAssign list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Timestamp</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for inquiryAssign in inquiryAssigns %}
            <tr>
                <td><a href="{{ path('inquiryassign_show', { 'id': inquiryAssign.id }) }}">{{ inquiryAssign.id }}</a></td>
                <td>{% if inquiryAssign.timestamp %}{{ inquiryAssign.timestamp|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('inquiryassign_show', { 'id': inquiryAssign.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('inquiryassign_edit', { 'id': inquiryAssign.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('inquiryassign_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
