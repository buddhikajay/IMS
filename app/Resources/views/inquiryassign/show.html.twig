{% extends 'base.html.twig' %}

{% block body %}
    <h1>InquiryAssign</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ inquiryAssign.id }}</td>
            </tr>
            <tr>
                <th>Timestamp</th>
                <td>{% if inquiryAssign.timestamp %}{{ inquiryAssign.timestamp|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('inquiryassign_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('inquiryassign_edit', { 'id': inquiryAssign.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
